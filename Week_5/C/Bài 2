#include <bits/stdc++.h>
using namespace std;

int main()
{
    string a;
    cin >> a;
    
    unsigned int i = 0, j = a.length();
    
    while(i <= j){
        if(a[i] != a[j]){
            cout << "NO";
            return 0;
        }
        ++i;
        --j;
    }
    
    cout << "YES";
    return 0;
}
